public class Movie1
{
    private String name;
    private int year;
    private String genre;
    private double rating;
    private boolean isRrated;

    public Movie1(String name, int year, String genre, double rating, boolean isRrated)
    {
        this.name = name;
        this.year = year;
        this.genre = genre;
        this.rating = rating;
        this.isRrated = isRrated;
    }

    public String getName(){
        return name;
    }
    public String getGenre(){
        return genre;
    }
    public int getYear(){
        return year;
    }
    public double getRating(){
        return rating;
    }
    public boolean isRrated(){
        return isRrated;
    }
}
