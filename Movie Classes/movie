public class Movie
{
    private String title;
    private int year;
    private String genre;
    private double rating;

    public Movie(String title, int year, String genre, double rating)
    {
        this.title = title;
        this.year = year;
        this.genre = genre;
        this.rating = rating;
    }

    public String getTitle(){
        return title;
    }
    
    public String getGenre(){
        //Split genrene op.
        //TilfÃ¸j til genre-Arrayliste
        return genre;
    }
    
    public int getYear(){
        return year;
    }
    
    public double getRating(){
        return rating;
    }
    
    public boolean isRrated(){
        //Brug getGenre().
        //Sammenlign med de r-rated genre.
        //Return true, hvis der er et match.
        return false;
    }
}
