public class GuiDemo
{
    public static void main(String args[]){
        HandlingUser hu = new HandlingUser();
        User user = new User("Charlotte",20);
        User user1 = new User("Frederik",10);
        User user2 = new User("Michella",13);
        hu.AddNewUser(user);
        hu.AddNewUser(user1);
        hu.AddNewUser(user2);
        Model m = new Model();
        View v = new View();
        Controller c = new Controller(m, v);
    }
}
